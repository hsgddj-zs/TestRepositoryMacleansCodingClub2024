def triangleTest(a, b, c):
  if a+b>c and a+c>b and b+c>a:
    return True
    print(a, b, c, "can form a triangle")
  else:
    return False
    print(a, b, c, "cannot form a triangle")
  return triangleTest

a=int(input("Enter the first side of the triangle: "))
b=int(input("Enter the second side of the triangle: "))
c=int(input("Enter the third side of the triangle: "))
if triangleTest(a, b, c) == True:
  print(a, b, c, "can form a triangle")
else:
  print(a, b, c, "cannot form a triangle")

print( )

def Greetings(time, name):
  time = int(input("What time is it:"))
  name = input("What’s your name:")
  if 0<time<12:
    print("Good morning", name)
  elif 12<time<18:
    print("Good afternoon", name)
  elif 18<time<24:
    print("Good evening", name)
  else:
    print("Good day", name)
  return Greetings

a=time = int(input("What time is it:"))
b=name = input("What’s your name:")
Greetings(a, b)

print( )

def getEven(numsList):
  evenList = []
  for num in numsList:
    if num % 2 == 0:
      evenList.append(num)
  return evenList
print(getEven(numsList))

numsList = []
for num in range(10):
  numsList.append(int(input("Enter a number:")))
print(getEven(numsList))
