arrived = False
while not arrived:
    answer = input("Enter Password:")
    if answer == "135565":
        arrived = True
print("Correct Password")


x = input("Enter a number:")
y = input("Enter your name:")
for y in range(x):
    print(y)


a = input("Enter a positive integer:")
if a%range(10000000000000000000000000000)==0:
    print("prime number")



z = input("Enter a positive integer:")
arrived = False
while not arrived:
    if z < 0:
        print("Not a positive integer.")
